fun sayHi(first, last) {
  print "Hi, " + first + " " + last + "!";
}

sayHi("Dear", "Reader");

fun fib(n) {  
  if (n <= 1) return n;  
  return fib(n - 2) + fib(n - 1);
   } 

var num = 30;
print "Starting timer...";
var start = clock();
print num + "th digit is " + fib(num); 
var duration = clock() - start;
print "Duration: ";
print duration;
    

